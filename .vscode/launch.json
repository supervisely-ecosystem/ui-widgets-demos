{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Uvicorn",
			"type": "python",
			"request": "launch",
			"module": "uvicorn",
			"args": [
				"misc.trained_models_table.src.main:app",
				"--host",
				"0.0.0.0",
				"--port",
				"8001",
				"--ws",
				"websockets",
				"--reload"
			],
			"jinja": true,
			"justMyCode": true,
			"env": {
				"PYTHONPATH": "${workspaceFolder}:${PYTHONPATH}",
				"LOG_LEVEL": "DEBUG"
			}
		}
	]
}
